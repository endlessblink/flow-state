{
  "name": "codebase-health-auditor",
  "version": "1.0.0",
  "description": "Unified dead code detection and cleanup tool for Vue 3 + TypeScript projects",
  "type": "module",
  "main": "scripts/orchestrator.js",
  "scripts": {
    "audit": "node scripts/orchestrator.js audit",
    "execute": "node scripts/orchestrator.js execute",
    "detect": "node scripts/orchestrator.js detect"
  },
  "dependencies": {
    "chalk": "^5.3.0",
    "commander": "^12.0.0",
    "glob": "^10.3.0",
    "yaml": "^2.3.0"
  },
  "peerDependencies": {
    "knip": ">=5.0.0",
    "depcheck": ">=1.4.0",
    "typescript": ">=5.0.0",
    "@vue/compiler-sfc": ">=3.4.0"
  },
  "peerDependenciesMeta": {
    "knip": {
      "optional": true
    },
    "depcheck": {
      "optional": true
    },
    "@vue/compiler-sfc": {
      "optional": true
    }
  },
  "keywords": [
    "dead-code",
    "unused-code",
    "code-cleanup",
    "vue",
    "typescript",
    "knip",
    "depcheck"
  ],
  "author": "Claude Code",
  "license": "MIT"
}
